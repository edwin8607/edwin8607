<%- model_class = Ingeniero -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombrecompleto) %>:</strong></dt>
  <dd><%= @ingeniero.nombrecompleto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dependencia) %>:</strong></dt>
  <dd><%= @ingeniero.dependencia %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estudios) %>:</strong></dt>
  <dd><%= @ingeniero.estudios %></dd>
  <dt><strong><%= model_class.human_attribute_name(:proyectos_id) %>:</strong></dt>
  <dd><%= @ingeniero.proyectos_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.Atras")),
              proyecto_ingenieros_path(@proyecto), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.Editar")),
              edit_proyecto_ingeniero_path(@proyecto,@ingeniero), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.Eliminar")),
              ingeniero_path(@ingeniero),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Esta seguro?')) },
              :class => 'btn btn-danger' %>
</div>
